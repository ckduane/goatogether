<div class='journey-show'>
  <h2><%= @journey.name %></h2>
  <div class="journey-details">
    <p><%= format_time(@journey.start_time) %> - <%= format_time(@journey.end_time) %></p>
  </div>

<div class="all-tweets">
  <div class='tweet-details'>
    <div class='tweet-user'>
      <div class="tweet-user-left">
        <div class="twitter-avatar"></div>
        <div class="tweet-name-username">
          <div class="twitter-name"></div>
          <div class="twitter-username"></div>
        </div>
      </div>
      <div class="tweet-user-right">
        <span class="twitter-icon"></span>
      </div>
    </div>
    <div class='tweet-content'>
    <div class='tweet-picture'></div>
      <div class="twitter-text"></div>
      <div class="twitter-date"></div>
    </div>
  </div>
</div>
  <div class='hide'>
   <div class="results-data hidden" aria-hidden="true"><%=sanitize(@combined_results)%></div>
  </div>
</div>
</div>
