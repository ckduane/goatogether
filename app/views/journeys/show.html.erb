<h2><%= @journey.name %></h2>
<p><%= format_time(@journey.start_time) %> - <%= format_time(@journey.end_time) %></p>
<p><%@result.each do |result|%></p>
<%=result.text%>
<%end%>


<%
=begin %>
<% @posts.each do |post| %>
  <% if post.provider == twitter %>
    <p><i class="fa fa-twitter-square" aria-hidden="true"></i>
    <% post.content %></p>
  <% else %>
    <p><i class="fa fa-instagram" aria-hidden="true"></i>
    <% post.content %></p>
  <% end %>
<% end %>
<%
=end %>
